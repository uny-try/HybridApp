<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <link rel="stylesheet" href="_content/ConnectFour.Shared/bootstrap/bootstrap.min.css" />
    <link rel="stylesheet" href="_content/ConnectFour.Shared/app.css" />
    <link rel="stylesheet" href="ConnectFour.Web.styles.css" />
    <link rel="icon" type="image/png" href="_content/ConnectFour.Shared/favicon.png" />
    <HeadOutlet @rendermode="InteractiveServer" />
</head>

<body>
    <Router AppAssembly="@typeof(App).Assembly">
        <Found Context="routeData">
            <RouteView RouteData="@routeData" DefaultLayout="typeof(ConnectFour.Shared.Layout.MainLayout)" />
        </Found>
        <NotFound>
            <h1>Not found</h1>
        </NotFound>
    </Router>
    <script src="_framework/blazor.web.js"></script>
</body>

</html>
